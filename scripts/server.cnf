[ req ]
default_bits       = 2048
prompt             = no
default_md         = sha256
distinguished_name = req_distinguished_name
req_extensions     = req_ext

[ req_distinguished_name ]
countryName            = IE
stateOrProvinceName    = GALWAY
localityName   = GALWAY CITY
organizationName       = .
commonName             = testserver

[ req_ext ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = testcertserver.com
DNS.2 = testcertserver.com